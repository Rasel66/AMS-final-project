<div class="container">
  <div class="row justify-content-center">
    <div class="col-8">
      <table class="table text-center text-white table-hover">
        <thead>
          <tr>
            <td class="text-center fs-4 mb-2 ">Course Name</td>
          </tr>
        </thead>
        <tbody>
          <% courses.forEach(course=> { %>
            <tr>
              <td>
                <a href="/attendance/course-id/<%= course._id %>" class="btn btn-link text-white" style="text-decoration: none; font-size: 18px;">
                  <%= course.name %>
                </a>
              </td>
            </tr>
            <% }); %>
        </tbody>
      </table>
    </div>
  </div>
</div>